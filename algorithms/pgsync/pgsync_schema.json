[
  {
    "database": "vikesplace",
    "index": "users",
    "nodes": {
      "table": "Users",
      "columns": [
        "user_id",
        "username",
        "joining_date"
      ]
    }
  },
  {
    "database": "vikesplace",
    "index": "listings",
    "plugins": ["Geometry"],
    "nodes": {
      "table": "Listings",
      "columns": [
        "listing_id",
        "seller_id",
        "buyer_username",
        "title",
        "price",
        "lat_long",
        "location",
        "status",
        "listed_at",
        "last_updated_at",
        "category"
      ],
      "transform": {
        "mapping": {
          "lat_long": {
            "type": "geo_point"
          }
        }
      }
    }
  }
]